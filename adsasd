MainSection:CreateButton("Gun Rgb", function() -- Button Name In Brackets
   print("Made By Maxlaser.")  local c = 1 function zigzag(X)  return math.acos(math.cos(X * math.pi)) / math.pi end game:GetService("RunService").RenderStepped:Connect(function()  if game.Workspace.Camera:FindFirstChild('Arms') then   for i,v in pairs(game.Workspace.Camera.Arms:GetDescendants()) do    if v.ClassName == 'MeshPart' then      v.Color = Color3.fromHSV(zigzag(c),1,1)     c = c + .0001    end   end  end end)
end)
MainSection:CreateButton("Aimbot", function() -- Button Name In Brackets
   print("Made By Maxlaser.")  loadstring(game:HttpGet("https://pastebin.com/raw/1Gp9c57U"))()
   end)
   MainSection:CreateButton("Noclip", function() -- Button Name In Brackets
   print("Made By Maxlaser.")  loadstring(game:HttpGet"https://raw.githubusercontent.com/IlikeScript1234/Roblox-maxlaser-1/main/Noclip")()
